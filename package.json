{
  "name": "bda",
  "version": "1.0.0",
  "description": "This project takes SoundCloud songs (via URL) and returns a BPM using soundwave analysis",
  "main": "src/index.tsx",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mrlucciola/bpm-detection-algorithm.git"
  },
  "author": "Rocco Lucciola",
  "license": "MIT",
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "private": true,
  "bugs": {
    "url": "https://github.com/mrlucciola/bpm-detection-algorithm/issues"
  },
  "homepage": "https://github.com/mrlucciola/bpm-detection-algorithm#readme",
  "scripts": {
    "start": "react-scripts start",
    "mb-move-assets": "cp -r ./assets/* ./",
    "mb-move-new-build": "mv -f ./build/* ./ && rm -rf ./build",
    "mb-remove-old-build": "rm -rf ./static ./assets ./images",
    "mb-manage-new-build": "yarn mb-remove-old-build && yarn mb-move-new-build && yarn mb-move-assets",
    "build": "react-scripts build",
    "deploy": "yarn build && yarn mb-manage-new-build"
  },
  "dependencies": {
    "@emotion/react": "^11.10.5",
    "@emotion/styled": "^11.10.5",
    "@mui/icons-material": "^5.10.16",
    "@mui/material": "^5.10.17",
    "@mui/types": "^7.2.2",
    "mobx": "^6.6.1",
    "mobx-react-lite": "^3.4.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.3.0",
    "react-scripts": "^5.0.1"
  },
  "devDependencies": {
    "@types/node": "^18.7.11",
    "@types/react": "^18.0.17",
    "@types/react-dom": "^18.0.6",
    "typescript": "^4.7.4"
  }
}
